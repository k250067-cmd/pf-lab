int main(){
int n;
int arr[100];
int c;
 printf("input how many number you want to enter");
    scanf("%d", &c);
for (int i = 0; i < c; i++)
{
 printf("inpiut a number to ");   
scanf("%d", &arr[i]);
}
int odd = 0;
int even = 0;
for (int i = 0; i < c; i++)
{
    if (i%2==0)
    {
     even++;
    }
    else {
        
     odd++;
    }  
}
printf("num of enen num %d", even);
printf("num of odd num %d", odd);
return 0;
}



q6
#include <stdio.h> 
int main(){
int n;
int c;
printf("give me a num to make multiplicztion table");
scanf("%d", &n);
int counter =1;
do
{
    c=n*counter;
    printf("%d\n", c);
    counter++;
} while (counter<=10);
return 0;
}



q5
#include<stdio.h>
int main() {
    int num;
    int sum =0;
    printf("input a num of factorial");
    scanf("%d", &num);
    int n = num-1;
    
    for (int i = num; i > 0; i--)
    {
        sum += num*n;

        n--;
    }
    printf("factorial of %d is %d\n",num,sum);
    for (int i = 1; i < num; i++)
    {
        printf("%d\n",i);
    }
    
    return 0;
}



q8.
#include <stdio.h>

int main() {
    int n, i, j;
    long long fact = 1, fact2n, factn, factn1, catalan;

    printf("Enter how many Catalan numbers you want ");
    scanf("%d", &n);

    printf("First %d Catalan numbers are\n", n);


    for (i = 0; i < n; i++) {
        
     
        fact2n = 1;
        for (j = 1; j <= 2 * i; j++) {
            fact2n = fact2n * j;
        }

      
        factn = 1;
        for (j = 1; j <= i; j++) {
            factn = factn * j;
        }

        
        factn1 = 1;
        for (j = 1; j <= i + 1; j++) {
            factn1 = factn1 * j;
        }

        catalan = fact2n / (factn1 * factn);

        printf("%d ", catalan);
    }

    printf("\n");
    return 0;
}
