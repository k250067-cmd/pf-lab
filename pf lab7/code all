#include <stdio.h> q2

int main() {
    float power[] = {120.5, 135.8, 110.2, 142.6, 128.4, 150.0};
    int size = sizeof(power) / sizeof(power[0]);

    float sum = 0;

    for (int i = 0; i < size; i++) {
        sum += power[i];
    }

    
    float average = sum / size;


    printf("Total power usage %.2f watts\n", sum);
    printf("Average hourly usage %.2f watts\n", average);

    return 0;
}
