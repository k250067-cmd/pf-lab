#include<stdio.h> q8
int main(){
    char arr[100]="abd";
    char brr[100]="abc";
    for (int i = 0; arr[i] != '\0' && brr[i] != '\0'; i++)
    {
     
     
        if (arr[i]!=brr[i])
        {
            printf("mismatched words on index %d\n", i);
        }
        else{
            printf("all same inputs\n");
        }
     
     
    }
    return 0;
}



