    if (a>b){
    a=a+b;
    b = a-b;
    a= a-b;
    }
    else {
            printf("invalid");
    return 0;
    }
        printf("give me a value of b after swapping is %d\n", b);
                printf("give me a value of a after swapping is %dn", a);


    
    
